{
  "name" : "rampage-php/auth",
  "description" : "Auth framework based on ZF2 / rampage-php",
  "require" : {
    "zendframework/zend-authentication" : ">=2.2, < 3.0",
    "zendframework/zend-servicemanager" : ">=2.2, < 3.0",
    "zendframework/zend-eventmanager" : ">=2.2,<3.0"
  },
  "require-dev" : {
    "phpunit/phpunit" : "3.7.*",
    "zendframework/zend-mvc" : ">=2.2,<3.0",
    "zendframework/zend-view" : ">=2.2,<3.0"
  },
  "authors" : [ {
    "name" : "Axel Helmert",
    "email" : "staff@pro-unreal.de",
    "role" : "lead-dev"
  } ],
  "keywords" : [ "zf2", "rampage", "authentication" ],
  "support" : {
    "issues" : "https://github.com/tux-rampage/rampage-auth/issues",
    "source" : "https://github.com/tux-rampage/rampage-auth"
  },
  "extra" : {
    "branch-alias" : {
      "dev-1.0.x" : "1.0.x-dev"
    }
  },
  "autoload" : {
    "psr-0" : {
      "rampage\\auth\\" : "library"
    }
  },
  "license" : "GPL-3.0+",
  "type" : "library",
  "homepage" : "https://github.com/tux-rampage/rampage-auth"
}
